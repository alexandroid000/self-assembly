<launch>
 <include file="$(find gazebo_ros)/launch/empty_world.launch">     
    <arg name="world_name" value="$(find weaselball_gazebo)/worlds/simulate.world"/>
	<arg name="gui" value="true"/>
	<arg name="paused" value="true"/>
  </include>  


<group ns="weasel0">
<include file="$(find weaselball_gazebo)/launch/include/one_robot.launch">
<arg name="robot_name" value="swarmbot0"/>
<arg name="init_pose" value="-x -0.018791 -y 0.052855 -z 0.03 -R 1.1 -P 1.2 -Y 1.3"/>
</include>
</group>

<group ns="weasel1">
<include file="$(find weaselball_gazebo)/launch/include/one_robot.launch">
<arg name="robot_name" value="swarmbot1"/>
<arg name="init_pose" value="-x 0.018791 -y -0.052855 -z 0.03 -R 0.397 -P 0.5015 -Y 0.37 "/>
</include>
</group>

<group ns="mount0">
<include file="$(find weaselball_gazebo)/launch/include/one_2_mount.launch">
<arg name="robot_name" value="mount0"/>
<arg name="init_pose" value="-x -0.0 -y -0.0 -z 0.01"/>
</include>
</group>

</launch>
